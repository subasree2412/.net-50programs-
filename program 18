using System;
using System.IO;

class Program
{
    static void Main()
    {
        string filePath = "example.txt";

        // Writing to the file
        File.WriteAllText(filePath, "Hello, .NET 5!");

        // Reading from the file
        string content = File.ReadAllText(filePath);
        Console.WriteLine("File Content: " + content);
    }
}
